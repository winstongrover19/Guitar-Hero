public class GuitarString{
private Queue<Integer> ringBuffer = new LinkedList();
private static final double SAMPLE_RATE = StdAudio.SAMPLE_RATE;

public GuitarString(double frequency){
int n = Math.round(SAMPLE_RATE / frequency);
if(frequency <= 0 || n < 2)
throw new IllegalArgumentException;

for(int i = 0; i < n; i++)
ringBuffer.add(0);
 

}

}
